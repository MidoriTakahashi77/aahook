{
  "name": "ocean",
  "version": "1.0.0",
  "description": "Ocean theme with blue and cyan gradients",
  "author": "aahook",
  "colors": {
    "mode": "line",
    "rules": [
      {
        "match": { "start": 0, "end": 1 },
        "color": { "fg": "#001F3F" }
      },
      {
        "match": { "start": 2, "end": 3 },
        "color": { "fg": "#003D7A" }
      },
      {
        "match": { "start": 4, "end": 5 },
        "color": { "fg": "#0074D9" }
      },
      {
        "match": { "start": 6, "end": 7 },
        "color": { "fg": "#39CCCC" }
      },
      {
        "match": { "start": 8, "end": 9 },
        "color": { "fg": "#7FDBFF" }
      },
      {
        "match": { "start": 10, "end": 99 },
        "color": { "fg": "#B0E0E6" }
      }
    ]
  }
}